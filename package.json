{
  "name": "@caido-community/dev",
  "version": "0.0.3",
  "description": "Development tools for building Caido plugins",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "bin": {
    "caido-dev": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
