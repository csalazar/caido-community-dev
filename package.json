    "jiti": "2.4.2",
    "jszip": "3.10.1",
    "ws": "8.18.0",
    "zod": "3.24.1"
  },
  "peerDependencies": {
    "vite": "^6.0.0",
    "tsup": "^8.0.0"
  },
  "devDependencies": {
    "@types/express": "5.0.0",
    "@types/node": "22.10.2",
    "@types/ws": "8.5.13",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
    "vitest": "2.1.8",
